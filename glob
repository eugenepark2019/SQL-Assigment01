select trackid,name from tracks where name glob 'Man*';
Select trackid,name from tracks where name glob '*Man';
select trackid,name from tracks where name glob'?ere*';
select trackid,name from tracks where name glob '*[1-9]*';
select trackid,name from tracks where name glob '*[^1-9]*';
select trackid,name from tracks where name glob '*[1-9]';
